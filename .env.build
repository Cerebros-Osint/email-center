# Build-time environment variables (safe defaults)
# These are ONLY used during build, overridden at runtime by Render

# Fake database URL for build (won't be used)
DATABASE_URL="postgresql://localhost:5432/build"

# Fake Redis URL for build (won't be used)
REDIS_URL="redis://localhost:6379"

# Dummy secrets for build
SESSION_SECRET="build-time-secret-not-used-in-production-32chars"
ENCRYPTION_KEY="0123456789abcdef0123456789abcdef0123456789abcdef0123456789abcdef"

# Skip validations during build
SKIP_ENV_VALIDATION="true"
NODE_ENV="production"
